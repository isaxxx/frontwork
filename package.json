    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "cpx": "^1.5.0",
    "ejss": "^1.2.3",
    "es6-object-assign": "^1.1.0",
    "eslint": "^5.8.0",
